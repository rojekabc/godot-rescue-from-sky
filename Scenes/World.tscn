	Game.verbose(\"Transport completed from \"
			+ Game.playerDefinitions[source.ownerIdx].name
			+ \".\" + Game.structureDefinitions[source.type].name
			+ \" send \" + Game.resourceDefinitions[resource].name
			+ \" to \" + Game.playerDefinitions[target.ownerIdx].name
			+ \".\" + Game.structureDefinitions[target.type].name)
	queue_free()
	pass
