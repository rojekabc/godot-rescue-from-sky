resource_name = "Structure"
script/source = "extends StaticBody2D

const HALF_SIZE = 30
const FULL_SIZE = 60

# type of Game.STRUCTURE enum
var type
var ownerIdx

var consumes = {}
var produces = {}

# structure specific data
var data = {}

#### Interface of targeting ####
var targeting = []
#### --- END --- ####

# Register consumer, which will receive produced resource
func register_consumer(resource, consumer):
	if produces.has(resource):
		var produce = produces[resource]
		produce.consumers.append(consumer)
	pass

func random_position(ownerIdx):
	position.x = HALF_SIZE + Game.MAP_WIDTH/2*ownerIdx + randi() % (Game.MAP_WIDTH/2 - FULL_SIZE)
	position.y = HALF_SIZE + randi() % (Game.MAP_HEIGHT - FULL_SIZE)
	pass

func assign_type(strType, timer):
	var strDef = Game.structureDefinitions[strType]
	type = strDef.type
	$Acronym.text = strDef.acronym
	timer.connect('timeout', self, '_timeout')
	for resource in strDef.consumes:
		_consumed_resource_add(resource)
	for resource in strDef.produces:
		_produced_resource_add(resource)
	_call_type_function('create_data')

func get_type():
	return type

func get_name():
	return Game.playerDefinitions[ownerIdx].name + '.' + Game.structureDefinitions[type].name
	
func _ready():
	pass

func get_corners():
	var result = []
	result.append(Vector2(position.x-HALF_SIZE, position.y-HALF_SIZE))
	result.append(Vector2(position.x-HALF_SIZE, position.y+HALF_SIZE))
	result.append(Vector2(position.x+HALF_SIZE, position.y-HALF_SIZE))
	result.append(Vector2(position.x+HALF_SIZE, position.y+HALF_SIZE))
	return result

func consumed_resource_put(resource):
	var consumedResource = consumes[resource]
	consumedResource.has = true
	consumedResource.wait = false

func consumed_resource_wait(resource):
	var consumedResource = consumes[resource]
	consumedResource.wait = true

func consumed_resource_may_send(resource):
	var consumedResource = consumes[resource]
	return not (consumedResource.has or consumedResource.wait)

func _produced_resource_distribute(produce):
	for consumer in produce.consumers:
		if not consumer.consumed_resource_may_send(produce.resource):
			continue
		Game.verbose(
			get_name()
			+ \" send \" + Game.resourceDefinitions[produce.resource].name
			+ \" to \" + consumer.get_name())
		var transport = Game.getWorld().transport_start(self, consumer, produce.resource)
		produce.has = false
		consumer.consumed_resource_wait(produce.resource)
		break

func _produced_resource_produce_tick(produce):
	produce.timeout -= 1
	if produce.timeout == 0:
		produce.has = true
		produce.producing = false
		Game.verbose(
			get_name()
			+ \" produced \" + Game.resourceDefinitions[produce.resource].name)
	pass

# check that has resources to start produce
func _produced_resource_has_resources(produce):
	var result = true
	var resDef = Game.resourceDefinitions[produce.resource]
	for consumedResource in resDef.consumes:
		if not consumes[consumedResource].has:
			result = false
	return result

func _produced_resource_start_production(produce):
	# consume resources
	var resDef = Game.resourceDefinitions[produce.resource]
	for consumedResource in resDef.consumes:
		consumes[consumedResource].has = false
	# start produce
	produce.producing = true
	produce.timeout = resDef.timeout
	Game.verbose(
		get_name()
		 + \" start production of \" + Game.resourceDefinitions[produce.resource].name)

func _produced_resource_timeout(produce):
	if produce.has:
		_produced_resource_distribute(produce)
	elif produce.producing:
		_produced_resource_produce_tick(produce)
	elif _produced_resource_has_resources(produce):
		_produced_resource_start_production(produce)
	pass

func _call_type_function(function, args=null):
	var strName = Game.structureDefinitions[type].name
	var callName = '_' + strName + '_' + function
	if has_method(callName):
		if args: call(callName, args)
		else: call(callName)

func pressed(world):
	_call_type_function('pressed', world)
	pass

func _timeout():
	for produce in produces.values(): _produced_resource_timeout(produce)
	_call_type_function('timeout')
	pass

func _Airport_create_data():
	data.planes = []
	pass

func _Airport_timeout():
	if not consumes[Game.AIRPLANE].has:
		return
	var assignPlaneCalls = []
	var planeCount = _Airport_count_planes()
	
	if planeCount[Game.FIGHTER] < Game.CONFIGURATION.airportFighterLimit:
		assignPlaneCalls.append('_Airport_add_fighter')
	if planeCount[Game.BOMBER] < Game.CONFIGURATION.airportBomberLimit:
		assignPlaneCalls.append('_Airport_add_bomber')
		
	if assignPlaneCalls.empty():
		return
		
	call(assignPlaneCalls[randi() % assignPlaneCalls.size()])
	consumes[Game.AIRPLANE].has = false
	pass

func _Airport_count_planes():
	var fighterCount = 0
	var bomberCount = 0
	for plane in data.planes:
		if plane.type == Game.FIGHTER: fighterCount += 1
		if plane.type == Game.BOMBER: bomberCount += 1
	return {
			Game.FIGHTER : fighterCount,
			Game.BOMBER : bomberCount
		}

func _Airport_pressed(world):
	world.ui_squad_panel(self)

func _Airport_add_fighter():
	data.planes.append({type=Game.FIGHTER})

func _Airport_add_bomber():
	data.planes.append({type=Game.BOMBER})

func _consumed_resource_add(resource):
	consumes[resource] = {
		resource = resource,
		has = false,
		supplier = null,
		# transport start, waiting for resource
		wait = false
	}

func _produced_resource_add(resource):
	var resourceDefinition = Game.resourceDefinitions[resource]
	produces[resource] = {
		resource = resource,
		producing = false,
		has = false,
		timeout = resourceDefinition.timeout,
		consumers = [],
		lastConsumer = null
	}
"
